{
  "name": "uptrace/uptrace",
  "description": "OpenTelemetry PHP distribution for Uptrace",
  "type": "library",
  "require": {
    "open-telemetry/api": "1.2.3",
    "open-telemetry/context": "1.1.0",
    "open-telemetry/sdk": "1.2.2",
    "open-telemetry/exporter-otlp": "1.2.1",
    "php-http/guzzle7-adapter": "1.1.0",
    "google/protobuf": "3.25.6",
    "open-telemetry/contrib-aws": "1.0.0beta12"
  },
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Uptrace\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Vladimir Mihailenco",
      "email": "vladimir.webdev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}
