{
  "name": "uptrace/uptrace",
  "description": "OpenTelemetry PHP distribution for Uptrace",
  "type": "library",
  "require": {
    "open-telemetry/api": "^1-beta",
    "open-telemetry/context": "^1-beta",
    "open-telemetry/sdk": "^1-beta",
    "open-telemetry/exporter-otlp": "^1-beta",
    "php-http/guzzle7-adapter": "1.0.0",
    "google/protobuf": "dev-master",
    "open-telemetry/contrib-aws": "dev-main"
  },
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Uptrace\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Vladimir Mihailenco",
      "email": "vladimir.webdev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
